<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="600.0" prefWidth="800.0" stylesheets="@../style/main.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fxapps.kieserverclient.controllers.TemplateController">
   <bottom>
      <HBox alignment="CENTER" prefHeight="49.0" prefWidth="800.0" spacing="25.0" BorderPane.alignment="CENTER">
         <children>
          <Button mnemonicParsing="false" onAction="#goBack" prefHeight="39.0" prefWidth="44.0" text="←">
               <styleClass>
                  <String fx:value="defaultButton" />
                  <String fx:value="plus" />
               </styleClass>
            </Button>
            <Button mnemonicParsing="false" onAction="#goHome" prefHeight="40.0" prefWidth="72.0" text="Home">
               <styleClass>
                  <String fx:value="defaultButton" />
                  <String fx:value="plus" />
               </styleClass>
            </Button>
            <Button mnemonicParsing="false" onAction="#doLogout" text="Quit">
               <styleClass>
                  <String fx:value="danger" />
                  <String fx:value="defaultButton" />
               </styleClass>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
